    "detect-indent": "5.0.0",
    "got": "8.3.2",
    "inquirer": "6.0.0",
    "lodash": "4.17.10",
    "npm": "^3.10.6",
    "npm-check-updates": "2.14.2",
    "opener": "1.4.3",
    "semver": "5.5.0",
    "yargs": "12.0.1"
